{
  "name": "net.atos~forum~{{ forumVersion }}",
  "config": {
    "main" : "net.atos.entng.forum.Forum",
    "port" : 8024,
    "app-name" : "Forum",
    "app-address" : "/forum",
    "app-icon" : "forum-large",
    "host": "{{ host }}",
    "ssl" : {{ ssl }},
    "auto-redeploy": false,
    "userbook-host": "{{ host }}",
    "integration-mode" : "HTTP",
    "app-registry.port" : 8012,
    "mode" : "{{ mode }}",
    "entcore.port" : 8009,
      "publicConf": {
        {% if xitiSwitch is defined and xitiSwitch == 'true' %}
          "xiti": {
            "ID_SERVICE": {
              "default": 2
            }
          }
        {% endif %}
      }
  }
}