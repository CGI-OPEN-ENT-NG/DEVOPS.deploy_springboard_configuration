{
  "name": "net.atos~community~{{ communityVersion }}",
  "config": {
    "main" : "net.atos.entng.community.Community",
    "port" : 8078,
    "mongodb" : true,
    "app-name" : "Community",
    "app-address" : "/community",
    "app-icon" : "community-large",
    "host": "{{ host }}",
    "ssl" : {{ ssl }},
    "auto-redeploy": false,
    "userbook-host": "{{ host }}",
    "mode" : "{{ mode }}",
    "publicConf": {
      {% if xitiSwitch is defined and xitiSwitch == 'true' %}
        "xiti": {
          "ID_SERVICE": {
            "default": 2
          }
        }
      {% endif %}
    }
  }
}