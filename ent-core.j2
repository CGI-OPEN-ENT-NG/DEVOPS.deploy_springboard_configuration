{
  "assets-path": "{{ assetsPath }}",
  "services": [
    {
      "name": "io.vertx~mod-mongo-persistor~3.0.0",
      "waitDeploy": true,
      "worker": true,
      "multi-threaded": true,
      "config": {
        "address": "wse.mongodb.persistor",
        {% if seeds is defined and seeds %}
        "seeds": {{ seeds }},
        {% endif %}
        "host": "{{ mongoHost }}",
        "port": {{ mongoPort }},
        "db_name": "{{ dbName }}",
        "use_mongo_types": true,
        "pool_size": 10
      }
    },
    {
      "name": "fr.wseduc~mod-image-resizer~2.0.0",
      "worker": true,
      "multi-threaded": true,
      "waitDeploy": true,
      "config": {
        "address": "wse.image.resizer",
        "fs-flat": {{ flatFsStorage }},
        {% if swiftUri is defined and swiftUri %}
        "swift" : {
          "uri" : "{{ swiftUri }}",
          "container" : "{{ swiftContainer }}",
          "user" : "{{ swiftUser }}",
          "key" : "{{ swiftKey }}"
        }
        {% else %}
        "gridfs" : {
          {% if seeds is defined and seeds %}
          "seeds": {{ seeds }},
          {% endif %}
          "host": "{{ mongoHost }}",
          "port": {{ mongoPort }},
          "db_name": "{{ dbName }}",
          "pool_size": 10
        }
        {% endif %}
      }
    },
    {
      "name": "fr.wseduc~mod-zip~2.0.0",
      "waitDeploy": true,
      "worker": true,
      "config": {
        "address": "entcore.zipper"
      }
    },
    {
      "name": "fr.wseduc~mod-postgresql~1.0.0",
      "waitDeploy": true,
      "worker": true,
      "multi-threaded": true,
      "config": {
        "address": "sql.persistor",
        "url" : "{{ sqlUrl }}",
        "username" : "{{ sqlUsername }}",
        "password" : "{{ sqlPassword }}"
      }
    },
    {
      "name": "com.opendigitaleducation~mod-json-schema-validator~1.0.0",
      "waitDeploy": true,
      "worker": true,
      "multi-threaded": true,
      "config": {}
    },

    {% if webdavHost is defined and webdavHost %}
    {
      "name": "fr.wseduc~mod-webdav~1.0.0",
      "config": {
        "address" : "webdav"
        {% if 'true' == webdavCredential %}
        ,
        "credentials" : {
          "{{ webdavHost }}" : {
            "username" : "{{ webdavUsername }}",
            "password" : "{{ webdavPassword }}",
            "insecure" : {{ webdavInsecure }}
          }
        }
        {% endif %}
        }
      },
    {% endif %}

    {
      "name": "fr.wseduc~mod-sms-proxy~1.0.0",
      "waitDeploy": true,
      "worker": true,
      "config": {
        "address": "entcore.sms",
        "providers": {{ smsProviders }}
      }
    },
    {
      "name": "fr.wseduc~mod-pdf-generator~1.0.0",
      "waitDeploy": true,
      "worker": true,
      "config": {
        "address": "entcore.pdf.generator"
      }
    },
    {
      "name": "org.entcore~infra~{{ entcoreVersion }}",
      "waitDeploy": true,
      "config": {
        "port": 8001,
        "mode": "{{ mode }}",
        "auto-redeploy": false,
        "path-prefix": "infra",
        "sql": true,
        {% if signKey is defined and signKey %}
        "key": "{{ signKey }}",
        {% endif %}
        {% if contentSecurityPolicy is defined and contentSecurityPolicy %}
        "content-security-policy": "{{ contentSecurityPolicy }}",
        {% endif %}
        "cluster" : {{ cluster }},
        "skins": {{ skins }},
        {% if swiftUri is defined and swiftUri %}
        "swift" : {
          "uri" : "{{ swiftUri}}",
          "container" : "{{ swiftContainer }}",
          "user" : "{{ swiftUser }}",
          "key" : "{{ swiftKey }}"
        },
        {% elif fsStoragePath is defined and fsStoragePath %}
        "file-system" : {
          "path" : "{{ fsStoragePath }}",
          "flat" : {{ flatFsStorage }}
        },
        {% else %}
        "gridfs-address" : "wse.gridfs.persistor",
        {% endif %}
        {% if sharedConf is defined and shareConf %}
        "sharedConf": {{ sharedConf }},
        {% endif %}
        {% if emailConfig is defined and emailConfig %}
        "emailConfig": {{ emailConfig }},
        {% else %}
        "emailConfig": {
          "email": "{{ senderEmail }}",
          "host": "{{ host }}"
        },
        {% endif %}
        "eventConfig": {
        "user-blacklist" : []
        },
        "neo4jConfig" : {
          {% if serverUris is defined and serverUris %}
          "server-uris": {{ serverUris }},
          {% else %}
          "server-uri": "{{ neo4jUri }}/db/data/",
          {% endif %}
          "slave-readonly": {{ neo4jSlaveReadOnly }},
          "legacy-indexes" : [{ "for" : "node", "name" : "node_auto_index", "type" : "fulltext" }]
        }
      }
    },

